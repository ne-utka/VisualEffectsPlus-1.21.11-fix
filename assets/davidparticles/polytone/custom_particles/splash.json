{
  "render_type": "translucent",
  "rotation_mode": "look_at_xyz",
  "random_sprite": true,
  "offset": [0, 0, 0],
  "has_physics": true,
  "kill_on_contact": false,
  "kill_when_still": false,

  "initializer": {
    "lifetime": "10 + floor(rand() * 8)",
    "size": "0.12 + rand() * 0.16",
    "alpha": "0",
    "custom": "rand()"
  },

  "ticker": {
    "dx": "DX*0.92",
    "dz": "DZ*0.92",

   "dy": "(0.5 + CUSTOM * 0.5) * ( sin(pi * (AGE + 1) / LIFETIME) - sin(pi * AGE / LIFETIME) )",

    "alpha": "0.85 * min(max((LIFETIME - AGE) / 3, 0), 1)"
  }
}