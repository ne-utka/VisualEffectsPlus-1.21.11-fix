{
  "render_type": "translucent",
  "rotation_mode": "look_up",
  "offset": [0, 10, 0],
  "random_sprite": false,
  "has_physics": false,
  "kill_on_contact": false,
  "kill_when_still": false,
  "liquid_affinity": "non_liquids",
  "exclusion_radius": 1,
  "tick_rate": 1,
  "initializer": {
    "lifetime": "240",
    "size": "0.5 + rand()*0.4",
    "alpha": "0.0",
    "roll": "0 - pi/2",
    "custom": "6.28318 * rand()"
  },
  "ticker": {
    "alpha": "max(0, min(1, min(AGE/6, (LIFETIME - AGE)/6)))",
