    "dz": "0.1 * sin(0.03 * AGE + CUSTOM)",
    "dy": "0.05 * sin(0.12 * AGE + 0.5 * CUSTOM)",
    "roll": "(0 - pi/2) + 0.9 * DZ / max(0.0001, sqrt(DX*DX + DZ*DZ)) * smoothstep(AGE, 0, 10) + pi * ( (PLAYER_Y-10) >= POS_Y)"
  },
  "force_spawn": true
}
