{
  "initializer":{
    "lifetime": "100 + rand()*80",
    "size": "4",
    "alpha": "0",
    "roll": "rand() * 2 * pi",
    "custom": "rand()"
  },
      "ticker": {
        "alpha": "(0.35 + RAIN) * sin((pi / LIFETIME) * AGE) * min(max((sqrt(DISTANCE_SQUARED) - 5) / 10, 0), 1) * max(0, min(1, (30 - sqrt(DISTANCE_SQUARED)) / 5))",
        "roll": "(CUSTOM*2*pi) + sin(AGE*0.025 + CUSTOM*2*pi) * 0.25",
        "dx": "0",
        "dy": "sin(AGE*0.04 + CUSTOM*2*pi) * 0.01",
        "dz": "0",
        "remove_condition": "sqrt(DISTANCE_SQUARED) > 30"
        },
  "render_type": "additive_translucent",
  "liquid_affinity": "non_liquids",
  "exclusion_radius": 3,
  "has_physics": true,
  "kill_when_still": false,
  "lit": true,
  "light_level": 1,
  "offset": [0, 0, 0],
