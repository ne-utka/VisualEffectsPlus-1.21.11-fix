{
  "render_type": "additive_translucent",
  "rotation_mode": "movement_aligned",
  "random_sprite": true,
  "lit": true,
  "light_level": 8,
  "initializer": {
    "custom": "(DAY_TIME / 12000 * pi)",
    "lifetime": "(100 + rand() * 60)*1.5",
