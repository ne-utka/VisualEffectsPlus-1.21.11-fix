    "dx": "cos(CUSTOM)*0.00075",
    "dy": "sin(CUSTOM)*0.0035",
    "remove_condition": "(sqrt(DISTANCE_SQUARED) > 32) + ( ( PLAYER_Y - 1 ) > POS_Y)"
  },
  "offset": [
    0,
    0,
    0
  ],
  "has_physics": false,
  "kill_on_contact": false,
  "kill_when_still": false,
