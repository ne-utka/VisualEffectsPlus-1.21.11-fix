{
        "initializer":{
          "lifetime": "120 + rand()*40",
          "alpha": "1",
          "red": "0.75+rand()*0.25",
          "green": "0.75+rand()*0.25",
          "blue": "0.75+rand()*0.25",
          "size": "0",
          "custom": "gaussian()"
        },
        "ticker": {
            "dx": "((CUSTOM*0.1) * sin((0.01 * AGE)))*0.5 + RAIN*0.05",
            "dy": "((CUSTOM*0.05) * sin((0.05 * AGE))+0.01)*0.5",
            "dz": "((CUSTOM*0.1) * cos((0.01 * AGE)))*0.5",
            "size": "(CUSTOM*0.05 + 0.075) * min(max(AGE / 8, 0), 1) * min(max((LIFETIME - AGE) / 8, 0), 1)",
            "remove_condition": "sqrt(DISTANCE_SQUARED) > 30"
        },
        "random_sprite": true,
        "liquid_affinity": "non_liquids",
        "render_type": "translucent",
        "has_physics": true,
        "kill_on_contact": true,
        "kill_when_still": false,
        "offset": [0, 0, 0],
        "colormap": "davidparticles:colormap_sandstorm"
}